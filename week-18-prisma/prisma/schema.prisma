generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id       Int    @id @default(autoincrement())
  name     String
  age      Int
  password String
  city     String
  todo     todo[]
}

model todo {
  id     Int     @id @default(autoincrement())
  title  String
  status Boolean @default(false)
  userid Int
  user   User    @relation(fields: [userid], references: [id])
}
